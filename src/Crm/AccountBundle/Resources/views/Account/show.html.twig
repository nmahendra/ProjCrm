{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Account</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Createdate</th>
                <td>{{ entity.createDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Lastupdate</th>
                <td>{{ entity.lastUpdate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Create_user_id</th>
                <td>{{ entity.createuserid }}</td>
            </tr>
            <tr>
                <th>Modifiyeduserid</th>
                <td>{{ entity.modifiyedUserId }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('account') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('account_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
